<table class="lista">
  <tr>
    <th colspan="9">{{ title }}</th>
  </tr>
  <tr>
    <th>PERIODO</th>
    <th>BU</th>
    <th>OPORTUNIDAD</th>
    <th>ADJUDICACION</th>
    <th>INICIO</th>
    <th>FIN</th>
    <th>MONTO</th>
    <th>INGRESO MES FACT.</th>
    <th style="width: 25%;">ÃšLTIMA NOTA DE HISTORIAL</th>
  </tr>
  {% for data in results %}
  <tr class="tupla">
    <td>{{ data.MES }}-{{ data.ANNO }} </td>
    <td>{{ data.BU }}</td>
    <td>{{ data.OPORTUNIDAD }}</td>
    <td>{{ data.ADJUDICACION | date("d-m-Y") }}</td>
    <td>{{ data.INICIO | date("d-m-Y") }}</td>
    <td>{{ data.FIN | date("d-m-Y") }}</td>
    <td>${{ data.MONTO }}</td>
    <td>${{ data.SUMA | number_format(2,'.',',') }}</td>
    <td>{{-data.last_note_meeting-}}</td>
  </tr>
  {% endfor %}
</table>
